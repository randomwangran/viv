FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10000;

deltaT          1;

writeControl    runTime;

writeInterval   2000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    probes
        {
        type            probes;
        functionObjectLibs            ("libsampling.so");
        writeControl    timeStep;
        writeInterval   10;

        fields
        (
            Pacthdist Pp U k
        );

        probeLocations
        (
         (0.5 0 0)
        );

        }

    forces
    {
        type                forces;
        libs                ("libforces.so");
        writeControl        timeStep;
        writeInterval       50;
        patches             (wing);
        rho                 rhoInf;
        log                 true;
        rhoInf              1;//1
        CofR                (0 0 0);
    }
	
		forceCoeffs
   {
        type        forceCoeffs;
        //type      forces;

    functionObjectLibs ( "libforces.so" );

    outputControl   timeStep;
    timeInterval    50;

    log         yes;

    patches     ( "wing.*" );
    pName       p;
    UName       U;
    rho         rhoInf;      // Indicates incompressible
    log         true;
    rhoInf      1;           // Redundant for incompressible
    liftDir     (0 1 0);
    dragDir     (1 0 0);
    CofR        (0 0 0);  // Axle midpoint on ground
    pitchAxis   (0 0 1);
    magUInf     0.1;//0.03854;
    lRef        0.05;;        // Wheelbase length
    Aref        0.005;        // Estimated

    /*binData
    {
        nBin        20;          // output data into 20 bins
        direction   (1 0 0);     // bin direction
        cumulative  yes;
    }*/
    }
}


// ************************************************************************* //
